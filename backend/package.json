{
  "name": "estate-commission-backend",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "engines": {
    "node": ">=18"
  },
  "scripts": {
    "dev": "tsx watch src/server.ts",
    "build": "tsc -p tsconfig.json",
    "start": "node dist/server.js",
    "generate": "prisma generate",
    "prisma:seed": "tsx prisma/seed.ts",
    "test": "node --import tsx --test \"src/tests/unit/**/*.ts\"",
    "test:all": "node --import tsx --test",
    "test:watch": "node --import tsx --test --watch",
    "coverage": "c8 -r text -r html node --import tsx --test \"src/tests/unit/**/*.ts\""

  },
  "prisma": {
    "seed": "tsx prisma/seed.ts"
  },
  "dependencies": {
    "@prisma/client": "^6.16.1",
    "dotenv": "^16.4.5",
    "reflect-metadata": "^0.1.14",
    "tsyringe": "^4.8.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/node": "^22.18.3",
    "eslint": "^9.11.1",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.30.0",
    "prettier": "^3.3.3",
    "prisma": "^6.16.1",
    "tsx": "^4.19.2",
    "typescript": "^5.9.2"
  }
}
